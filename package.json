{
  "name": "@p12/contracts",
  "version": "v0.0.3",
  "license": "GPL-3.0",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "prepare": "husky install",
    "compile": "hardhat compile",
    "deploy": "hardhat deploy",
    "verify": "hardhat run ./tools/verify.ts",
    "export": "hardhat run ./tools/export.ts",
    "doc:gen": "hardhat docgen",
    "doc:dev": "docusaurus start",
    "doc:build": "docusaurus build",
    "flat": "hardhat flat",
    "prettier": "prettier -w .",
    "test": "hardhat test --deploy-fixture",
    "test:analysis": "REPORT_GAS=true hardhat test",
    "coverage": "HARDHAT_DEPLOY_FIXTURE=true hardhat coverage",
    "lint": "cspell \"**/*.sol\" \"**/*.ts\" && prettier -w . && eslint \"**/**.ts\" --fix && solhint \"contracts/**/*.sol\"",
    "estimate-size": "hardhat size-contracts --max-memory 8096"
  },
  "devDependencies": {
    "@algolia/client-search": "^4.9.1",
    "@docusaurus/core": "2.0.1",
    "@docusaurus/preset-classic": "2.0.1",
    "@ethersproject/abi": "^5.7.0",
    "@ethersproject/bytes": "^5.7.0",
    "@ethersproject/providers": "^5.7.2",
    "@mdx-js/react": "^1.6.22",
    "@metamask/eth-sig-util": "^4.0.1",
    "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers@0.3.0-beta.13",
    "@nomiclabs/hardhat-etherscan": "^3.1.0",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@openzeppelin/hardhat-upgrades": "^1.17.0",
    "@typechain/ethers-v5": "^7.2.0",
    "@typechain/hardhat": "^6.1.6",
    "@types/chai": "^4.3.0",
    "@types/fs-extra": "^9.0.13",
    "@types/mocha": "^9.0.0",
    "@types/node": "^12.20.40",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "chai": "^4.3.4",
    "cspell": "^5.15.1",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-promise": "^5.2.0",
    "ethereum-waffle": "^4.0.2",
    "ethers": "^5.7.2",
    "fs-extra": "^10.1.0",
    "hardhat": "2.12.6",
    "hardhat-contract-sizer": "^2.6.1",
    "hardhat-deploy": "^0.11.25",
    "hardhat-gas-reporter": "^1.0.6",
    "husky": "^7.0.4",
    "prettier": "^2.5.1",
    "prettier-plugin-sh": "^0.8.1",
    "prettier-plugin-solidity": "^1.0.0-beta.13",
    "prism-react-renderer": "^1.3.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "solhint": "^3.3.6",
    "solidity-coverage": "0.7.22",
    "solidity-docgen": "^0.6.0-beta.22",
    "ts-node": "^10.4.0",
    "tsort": "^0.0.1",
    "typechain": "^8.1.1",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.7.3",
    "@openzeppelin/contracts-upgradeable": "^4.7.3",
    "@openzeppelin/upgrades-core": "^1.19.1",
    "@p12/contracts-lib": "^0.0.5",
    "@tovarishfin/hardhat-yul": "^3.0.5",
    "@uniswap/v2-core": "^1.0.1",
    "@uniswap/v2-periphery": "^1.1.0-beta.0",
    "@uniswap/v3-core": "^1.0.1",
    "@uniswap/v3-periphery": "^1.4.3",
    "canonical-weth": "^1.4.0",
    "hardhat-preprocessor": "^0.1.5",
    "lodash": "^4.17.15"
  },
  "overrides": {
    "@nomiclabs/hardhat-waffle": {
      "@nomiclabs/hardhat-ethers": "$@nomiclabs/hardhat-ethers"
    },
    "@openzeppelin/hardhat-upgrades": {
      "@nomiclabs/hardhat-ethers": "$@nomiclabs/hardhat-ethers"
    }
  },
  "files": [
    "deployments"
  ]
}
